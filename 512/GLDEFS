

/*
	A material definition looks like this:

	material <flat|texture> <TEXTURENAME>
	{
		normal <NORMALMAP>
		specular <SPECULARMAP>
		specularlevel <float>
		glossiness <float>
		brightmap <BRIGHTMAP>
		[iwad]
		[thiswad]
		[disablefullbright]
	}

	Or for PBR materials:

	material <flat|texture> <TEXTURENAME>
	{
		normal <NORMALMAP>
		metallic <METALMAP>
		roughness <ROUGHMAP>
		ao <AMBIENTOCCLUSIONMAP>
		brightmap <BRIGHTMAP>
		[iwad]
		[thiswad]
		[disablefullbright]
	}

*/

material texture DOORTRAK
{
	normal "normalmaps/DOORTRAK.png"
	metallic "metallic/DOORTRAK.png"
	roughness "roughness/DOORTRAK.png"
	texture tex_heightmap "heightmaps/DOORTRAK.png"
	shader "shaders/texture/parallax3.fp"
	ao "specularmaps/white.png"

}


